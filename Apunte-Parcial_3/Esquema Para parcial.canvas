      "styleAttributes": {},
      "x": -4615,
      "y": 3208,
      "width": 225,
      "height": 70,
      "color": "4"
    },
    {
      "id": "fea4a763f40120f7",
      "type": "text",
      "text": "Características",
      "styleAttributes": {},
      "x": -4373,
      "y": 2978,
      "width": 200,
      "height": 50,
      "color": "4"
    },
    {
      "id": "b6747c69b7422570",
      "type": "text",
      "text": "Adjudicación",
      "styleAttributes": {},
      "x": -4353,
      "y": 3053,
      "width": 200,
      "height": 50,
      "color": "4"
    },
    {
      "id": "d7856533df55a226",
